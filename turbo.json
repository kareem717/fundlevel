{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**/*",
        ".next/**",
        "!.next/cache/**",
        "bin/*"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "build",
        "^build"
      ]
    },
    "install": {
      "cache": false,
      "dependsOn": [
        "^install"
      ]
    },
    "supabase:seed:sync": {
      "cache": false,
      "dependsOn": [
        "^supabase:seed:sync"
      ]
    },
    "supabase:seed:init": {
      "cache": false,
      "dependsOn": [
        "^supabase:seed:init"
      ]
    },
    "supabase:seed": {
      "cache": false,
      "dependsOn": [
        "^supabase:seed"
      ]
    },
    "test": {
      "cache": false,
      "dependsOn": [
        "^test"
      ]
    },
    "supabase:start": {
      "cache": false,
      "dependsOn": [
        "^supabase:start"
      ]
    },
    "supabase:stop": {
      "cache": false,
      "dependsOn": [
        "^supabase:stop"
      ]
    },
    "supabase:status": {
      "cache": false,
      "dependsOn": [
        "^supabase:status"
      ]
    },
    "supabase:restart": {
      "cache": false,
      "dependsOn": [
        "^supabase:restart"
      ]
    },
    "supabase:up": {
      "cache": false,
      "dependsOn": [
        "^supabase:up"
      ]
    },
    "supabase:diff": {
      "cache": false,
      "dependsOn": [
        "^supabase:diff"
      ]
    },
    "supabase:new": {
      "cache": false,
      "dependsOn": [
        "^supabase:new"
      ]
    },
    "supabase:reset": {
      "cache": false,
      "dependsOn": [
        "^supabase:reset"
      ]
    },
    "supabase:typegen": {
      "cache": false,
      "dependsOn": [
        "^supabase:typegen"
      ]
    },
    "openapi:gen": {
      "cache": false,
      "dependsOn": [
        "^openapi:gen"
      ],
      "outputs": [
        "openapi.yaml"
      ]
    },
    "sdk:gen": {
      "cache": false,
      "dependsOn": [
        "^sdk:gen"
      ]
    }
  }
}