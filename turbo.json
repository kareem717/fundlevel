{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**/*",
        ".next/**",
        "!.next/cache/**",
        "bin/*"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "build",
        "^build"
      ]
    },
    "install": {
      "cache": false,
      "dependsOn": [
        "^install"
      ]
    },
    "test": {
      "cache": false,
      "dependsOn": [
        "^test"
      ]
    },
    "db:start": {
      "cache": false,
      "dependsOn": [
        "^db:start"
      ]
    },
    "db:stop": {
      "cache": false,
      "dependsOn": [
        "^db:stop"
      ]
    },
    "db:status": {
      "cache": false,
      "dependsOn": [
        "^db:status"
      ]
    },
    "db:restart": {
      "cache": false,
      "dependsOn": [
        "^db:restart"
      ]
    },
    "db:up": {
      "cache": false,
      "dependsOn": [
        "^db:up"
      ]
    },
    "db:up-one": {
      "cache": false,
      "dependsOn": [
        "^db:up-one"
      ]
    },
    "db:down": {
      "cache": false,
      "dependsOn": [
        "^db:down"
      ]
    },
    "db:reset": {
      "cache": false,
      "dependsOn": [
        "^db:reset"
      ]
    },
    "openapi:gen": {
      "cache": false,
      "dependsOn": [
        "^openapi:gen:spec"
      ],
      "outputs": [
        "openapi.yaml"
      ]
    },
    "sdk:gen": {
      "cache": false,
      "dependsOn": [
        "^sdk:gen"
      ]
    }
  }
}