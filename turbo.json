{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "@fundlevel/web#build": {
      "dependsOn": ["@fundlevel/sdk#build"]
    },
    "@fundlevel/sdk#build": {
      "dependsOn": ["@fundlevel/api#build"]
    },
    "@fundlevel/api#build": {
      "dependsOn": ["@fundlevel/db#build"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "format": {
      "dependsOn": ["^format"]
    }
  }
}
