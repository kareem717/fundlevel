{
  "extends": "@fundlevel/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@fundlevel/db/*": [
        "./src/*"
      ]
    },
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}