{
  "name": "@fundlevel/db",
  "type": "module",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "db:start": "docker compose -f ../../docker-compose.yml up postgres",
    "db:stop": "docker compose -f ../../docker-compose.yml down postgres",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "biome lint .",
    "format": "biome format .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules dist"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "drizzle-zod": "^0.7.0",
    "postgres": "^3.4.5",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@fundlevel/typescript-config": "workspace:*",
    "drizzle-kit": "^0.30.5",
    "tsup": "^8.0.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js",
      "require": "./dist/validators/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js",
      "require": "./dist/schema/index.cjs"
    }
  }
}
