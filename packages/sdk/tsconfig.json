{
  "extends": "@fundlevel/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@fundlevel/db/*": [
        "./src/*"
      ]
    },
    "outDir": "./dist",
    "declaration": false,
    "declarationMap": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build"
  ]
}