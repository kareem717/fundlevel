{
  "name": "supabase",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "biome check .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "db:seed": "pnpm dlx tsx seed.ts",
    "db:seed:sync": "pnpm dlx @snaplet/seed sync",
    "db:seed:init": "pnpm dlx @snaplet/seed init",
    "db:start": "source .env && supabase start",
    "db:stop": "source .env && supabase stop",
    "db:status": "source .env && supabase status",
    "db:restart": "pnpm run db:stop && pnpm run db:start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "^0.98.0",
    "@types/pg": "^8.11.10",
    "pg": "^8.13.1"
  }
}
