{
  "name": "@fundlevel/supabase",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "biome check .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:restart": "bun run supabase:stop && bun run supabase:start",
    "supabase:seed": "tsx scripts/seed.ts",
    "supabase:seed:sync": "bunx @snaplet/seed sync",
    "supabase:seed:init": "bunx @snaplet/seed init",
    "supabase:up": "supabase migration up",
    "supabase:new": "supabase migration new",
    "supabase:reset": "supabase db reset",
    "supabase:diff": "supabase db diff",
    "supabase:typegen": "supabase gen types typescript --local > src/types/db.ts && bun supazod -i src/types/db.ts -o src/zod.ts -t zod.d.ts -s public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "^0.98.0"
  },
  "exports": {
    "./server": "./src/client/server.ts",
    "./types": "./src/types/index.ts",
    "./zod": "./src/zod.ts"
  },
  "dependencies": {
    "supazod": "^1.2.3"
  }
}
