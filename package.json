{
  "name": "fundlevel",
  "private": true,
  "scripts": {
    "build": "turbo build && turbo run openapi:gen && turbo run sdk:gen",
    "dev": "turbo dev",
    "start": "turbo run start",
    "test": "turbo run test",
    "install": "turbo run install",
    "supabase:reset": "turbo run supabase:reset",
    "supabase:up": "turbo run supabase:up",
    "supabase:seed": "turbo run supabase:seed",
    "supabase:seed:init": "turbo run supabase:seed:init",
    "supabase:seed:sync": "turbo run supabase:seed:sync",
    "supabase:start": "turbo run supabase:start",
    "supabase:stop": "turbo run supabase:stop",
    "supabase:status": "turbo run supabase:status",
    "supabase:restart": "turbo run supabase:restart",
    "supabase:diff": "turbo run supabase:diff",
    "supabase:new": "turbo run supabase:new",
    "supabase:typegen": "turbo run supabase:typegen",
    "openapi:gen": "turbo run openapi:gen",
    "sdk:gen": "turbo run sdk:gen",
    "sdk": "turbo run openapi:gen && turbo run sdk:gen",
    "format": "biome format --write ."
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.38",
  "workspaces": ["apps/*", "packages/*"],
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "turbo": "^2.4.4",
    "typescript": "^5.7.2"
  }
}
