{
  "name": "fundlevel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start",
    "clean": "turbo clean && rm -rf node_modules && rm -rf pnpm-lock.yaml",
    "db:generate": "pnpm --filter @fundlevel/db db:generate",
    "db:migrate": "pnpm --filter @fundlevel/db db:migrate",
    "db:push": "pnpm --filter @fundlevel/db db:push",
    "db:studio": "pnpm --filter @fundlevel/db db:studio",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@fundlevel/typescript-config": "workspace:*",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
