{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    // I dont know why but this breaks the web imports
    // "baseUrl": ".",
    // "paths": {
    //   "@/*": ["./src/*"]
    // },
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "allowJs": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true
  }
}
